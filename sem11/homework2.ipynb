init_printing()
start = -20.0
stop = 24.0
step = 0.1
def solve(x):
    y = -12*x**4*sin(cos(x)) - 18*x**3+5*x**2 + 10*x - 30
    return y
num_x = []
num_y = []
starts = start
while starts<stop:
    num_x.append(starts)
    starts+=step
for num in num_x:
    num_y.append(round(solve(num), 2))
